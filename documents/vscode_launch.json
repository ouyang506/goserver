    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Debug Remote",
            "type": "go",
            "request": "launch",
            "mode": "remote",
            "port": 2345,
            "host": "127.0.0.1",
            "program":"${fileDirname}",
            "remotePath":"/home/xxx/goserver/servers/gateserver",
            "showLog": true,
        },
                
        {
            "name": "Attach Remote",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1"
        }
    ]
}